<?xml version="1.0" encoding="UTF-8"?>

<faces-config version="2.0"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
	<managed-bean>
	<managed-bean-name>start</managed-bean-name>
		<managed-bean-class>gui.bean.HasieraBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>query</managed-bean-name>
		<managed-bean-class>gui.bean.QueryQuestionsBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>create</managed-bean-name>
		<managed-bean-class>gui.bean.CreateQuestionBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>register</managed-bean-name>
		<managed-bean-class>gui.bean.RegisterBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>login</managed-bean-name>
		<managed-bean-class>gui.bean.LoginBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>user</managed-bean-name>
		<managed-bean-class>gui.bean.UserBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>button</managed-bean-name>
		<managed-bean-class>gui.bean.Button</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>Bets.xhtml</display-name>
		<from-view-id>/Bets.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>queryQuestions</from-outcome>
			<to-view-id>/QueryQuestions.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Bets.xhtml</display-name>
		<from-view-id>/Bets.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>createQuestions</from-outcome>
			<to-view-id>/CreateQuestions.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>CreateQuestions.xhtml</display-name>
		<from-view-id>/CreateQuestions.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/Login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Bets.xhtml</display-name>
		<from-view-id>/Bets.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/Login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>QueryQuestions.xhtml</display-name>
		<from-view-id>/QueryQuestions.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/Login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>CreateQuestions.xhtml</display-name>
		<from-view-id>/CreateQuestions.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>register</from-outcome>
			<to-view-id>/Register.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Bets.xhtml</display-name>
		<from-view-id>/Bets.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>register</from-outcome>
			<to-view-id>/Register.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>QueryQuestions.xhtml</display-name>
		<from-view-id>/QueryQuestions.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>register</from-outcome>
			<to-view-id>/Register.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Register.xhtml</display-name>
		<from-view-id>/Register.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/Login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Login.xhtml</display-name>
		<from-view-id>/Login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>register</from-outcome>
			<to-view-id>/Register.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Register.xhtml</display-name>
		<from-view-id>/Register.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>registered</from-outcome>
			<to-view-id>/Bets.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Login.xhtml</display-name>
		<from-view-id>/Login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>loged</from-outcome>
			<to-view-id>/Bets.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>